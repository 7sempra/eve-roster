    "@opentelemetry/tracing": "^0.19.0",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "heapdump": "^0.3.15",
    "knex": "^0.95.6",
    "moment": "^2.29.1",
    "node-graceful": "^3.0.1",
    "node-schedule": "^2.0.0",
    "p-limit": "^3.1.0",
    "patch-package": "^6.4.7",
    "path-to-regexp": "^6.2.0",
    "pg": "^8.6.0",
    "progress-stream": "^2.0.0",
    "pug": "^3.0.2",
    "relaxed-json": "^1.0.3",
    "serve-favicon": "^2.5.0",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.2",
    "tmp": "^0.2.1",
    "unbzip2-stream": "^1.4.3",
    "underscore": "^1.13.1",
    "verror": "^1.10.0"
