    "webpack-dev-middleware": "^4.3.0",
    "webpack-hot-middleware": "webpack-contrib/webpack-hot-middleware#webpack5",
    "webpack-merge": "^5.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "test/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "engines": {
    "node": "14.17.0"
  }
}
