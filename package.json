    "@types/jest": "^26.0.23",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/node": "^14.17.0",
    "@types/node-schedule": "^1.3.1",
    "@types/pg": "^8.6.0",
    "@types/pg-pool": "^2.0.2",
    "@types/progress-stream": "^2.0.1",
    "@types/serve-favicon": "^2.5.2",
    "@types/sqlite3": "^3.1.7",
    "@types/svgo": "^2.3.0",
    "@types/terser-webpack-plugin": "^5.0.3",
    "@types/tmp": "^0.2.0",
    "@types/underscore": "^1.11.2",
