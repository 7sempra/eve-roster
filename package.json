{
  "name": "eve-roster",
  "version": "1.0.0",
  "private": true,
  "description": "EVE Roster",
  "author": "SOUND IT",
  "scripts": {
    "build": "yarn run build-server && webpack --config out/server/infra/build-client/webpack.prod.js",
    "build:dev": "yarn run build-server && webpack --config out/server/infra/build-client/webpack.dev.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --max-warnings=0",
    "build-server": "tsc",
    "clean": "rm -rf out/ && rm -rf static/dist/",
    "postinstall": "patch-package",
    "pretty-check": "prettier --check .",
    "start": "node out/server/bin/witness/witness.js",
    "test": "yarn run jest && yarn run pretty-check && yarn run lint",
    "updatedb": "node out/server/bin/updatedb.js",
    "watch-server": "tsc -w"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.2",
    "@opentelemetry/api": "^1.0.0-rc.3",
