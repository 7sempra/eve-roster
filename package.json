    "webpack-merge": "^5.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "resolver": "ts-jest-resolver",
    "roots": [
      "test/"
    ],
    "testEnvironment": "node",
    "testRegex": "(\\.|/)(test|spec)\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
