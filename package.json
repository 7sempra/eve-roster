    "build-server": "tsc",
    "clean": "rm -rf out/ && rm -rf static/dist/",
    "pretty-check": "prettier --check .",
    "start": "yarn node out/server/bin/witness/witness.js",
    "test": "yarn run jest -i && yarn run pretty-check && yarn run lint",
    "updatedb": "yarn node out/server/bin/updatedb.js",
    "watch-server": "tsc -w"
