    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/semantic-conventions": "^1.18.1",
    "@sentry/node": "^7.80.0",
    "axios": "^1.6.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "jose": "^5.1.0",
    "knex": "^3.0.1",
    "moment": "^2.29.4",
    "node-graceful": "^3.1.0",
    "node-schedule": "^2.1.1",
    "p-limit": "^5.0.0",
    "pg": "^8.11.3",
    "progress-stream": "^2.0.0",
    "pug": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.6",
    "tmp": "^0.2.1",
    "unbzip2-stream": "^1.4.3",
    "underscore": "^1.13.6",
    "verror": "^1.10.1"
  },
